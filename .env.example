# News Aggregator Environment Configuration
# Copy this file to .env and update with your settings

# Backend API Configuration
# Production backend URL
API_BASE_URL=https://soft09.tech

# API endpoint for submitting news articles
API_SUBMIT_ENDPOINT=/api/v2/parser/news/submit

# Enable/disable sending articles to backend API
# Set to 'true' to send articles to backend, 'false' to only save locally
SEND_TO_API=true
